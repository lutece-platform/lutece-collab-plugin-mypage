<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

    <name>mypage</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.1</version>
    <description>mypage plugin</description>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider>Mairie de Paris</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/mypage/mypage.png</icon-url>
    <copyright>Copyright 2001-2011 Mairie de Paris</copyright>
    <db-pool-required>1</db-pool-required>

    <!-- Specific plugin CSS stylesheet -->
    <css-stylesheets>
        <css-stylesheets-scope>xpage</css-stylesheets-scope>
        <css-stylesheet>mypage/mypage.css</css-stylesheet>
    </css-stylesheets>

    <!-- Applications -->
    <applications>
        <application>
            <application-id>mypage</application-id>
            <application-class>fr.paris.lutece.plugins.mypage.web.MyPageApp</application-class>
        </application>
    </applications>

    <admin-features>
        <admin-feature>
            <feature-id>MYPAGE_MANAGEMENT</feature-id>
            <feature-title>mypage.adminFeature.mypageManagement.name</feature-title>
            <feature-description>mypage.adminFeature.mypageManagement.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/mypage/ManageMyPage.jsp</feature-url>
            <feature-icon-url>images/admin/skin/plugins/mypage/mypage.png</feature-icon-url>
        </admin-feature>
    </admin-features>

</plug-in>